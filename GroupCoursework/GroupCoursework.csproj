<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="wwwroot\CSS\**" />
    <Compile Remove="wwwroot\HTML\**" />
    <Compile Remove="wwwroot\images\**" />
    <Compile Remove="wwwroot\Javascript\**" />
    <Content Remove="wwwroot\CSS\**" />
    <Content Remove="wwwroot\HTML\**" />
    <Content Remove="wwwroot\images\**" />
    <Content Remove="wwwroot\Javascript\**" />
    <EmbeddedResource Remove="wwwroot\CSS\**" />
    <EmbeddedResource Remove="wwwroot\HTML\**" />
    <EmbeddedResource Remove="wwwroot\images\**" />
    <EmbeddedResource Remove="wwwroot\Javascript\**" />
    <None Remove="wwwroot\CSS\**" />
    <None Remove="wwwroot\HTML\**" />
    <None Remove="wwwroot\images\**" />
    <None Remove="wwwroot\Javascript\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.6.2" />
  </ItemGroup>

</Project>
